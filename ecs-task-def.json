{
  "family": "myapp-task",
  "executionRoleArn": "arn:aws:iam::381491917220:role/nodeapp",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "testapp",
      "image": "REPLACE_IMAGE",
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80
        },
        {
          "containerPort": 3000,
          "hostPort": 3000
        }
      ],
      "essential": true
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "1024",
  "memory": "2048"
}
